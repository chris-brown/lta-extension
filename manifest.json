{
  "manifest_version": 3,
  "name": "LTA ranking extension",
  "description": "Adds ranking points to the entries page",
  "version": "1.0.3",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "minimum_chrome_version": "102",
  "omnibox": {
    "keyword": "lta rankings extension"
  },
  "content_scripts": [
    {
      "matches": ["https://competitions.lta.org.uk/sport/event.aspx*"],
      "js": ["content.js"]
    }
  ]
}
